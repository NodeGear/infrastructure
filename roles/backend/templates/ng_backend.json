{
	"redis_port": {{ redis_port }},
	"redis_host": "{{ redis_ip }}",
	"redis_key": "{{ redis_key }}",
	"bugsnag_key": "{{ bugsnag_key }}",
	"smtp": {
		"user": "{{ smtp_user }}",
		"pass": "{{ smtp_pass }}"
	},
	"db": "mongodb://{{ mongodb_auth_user }}:{{ mongodb_auth_pass }}@{{ mongo_ip }}:{{ mongodb_port }}/ng",
	"db_options": {
		"auto_reconnect": true,
		"native_parser": true,
		"server": {
			"auto_reconnect": true
		}
	},
	"server": {
		"id": "{{ server_id }}",
		"name": "{{ server_name }}",
		"price": {{ server_price }},
		"address": "{{ server_ip }}",
		"app_memory": {{ server_memory }}
	},
	"storage": {
		"enabled": true,
		"server": "http://{{ apps.fs.ip }}:{{ apps.fs.port }}",
		"auth": "{{ apps.fs.auth }}"
	},
	"statsd_ip": "{{ apps.graphite.ip }}",
	"statsd_port": {{ apps.graphite.statsd }}
}