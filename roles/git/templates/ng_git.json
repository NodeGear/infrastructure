{
	"redis_port": 6379,
	"redis_host": "{{ redis_ip }}",
	"redis_key": "{{ redis_key }}",
	"bugsnag_key": "{{ bugsnag_key }}",
	"db": "mongodb://{{ mongodb_auth_user }}:{{ mongodb_auth_pass }}@{{ mongo_ip }}:2017/ng",
	"networkDb": "mongodb://{{ mongodb_auth_network_user }}:{{ mongodb_auth_network_pass }}@{{ mongo_ip }}:2017/ngNetwork",
	"db_options": {
		"auto_reconnect": true,
		"native_parser": true,
		"server": {
			"auto_reconnect": true
		}
	},
	"networkDb_options": {
		"auto_reconnect": true,
		"native_parser": true,
		"server": {
			"auto_reconnect": true
		}
	},
	"gitolite": "/root/gitolite/"
}