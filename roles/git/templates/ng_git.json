{
	"redis_port": {{ redis_port }},
	"redis_host": "{{ redis_ip }}",
	"redis_key": "{{ redis_key }}",
	"bugsnag_key": "{{ bugsnag_key }}",
	"db": "mongodb://{{ mongodb_auth_user }}:{{ mongodb_auth_pass }}@{{ mongo_ip }}:{{ mongodb_port }}/ng",
	"db_options": {
		"auto_reconnect": true,
		"native_parser": true,
		"server": {
			"auto_reconnect": true
		}
	},
	"statsd_ip": "{{ apps.graphite.ip }}",
	"statsd_port": {{ apps.graphite.statsd }}
}