{
	"redis_port": 6379,
	"redis_host": "{{ redis_ip }}",
	"redis_key": "{{ redis_key }}",
	{% if use_analytics == True %}
	"use_analytics": true,
	{% else %}
	"use_analytics": false,
	{% endif %}
	{% if is_ssl == True %}
	"is_ssl": true,
	{% else %}
	"is_ssl": false,
	{% endif %}
	"mongolab_endpoint": "{{ mongolab_endpoint }}",
	"mongolab_sso_salt": "{{  mongolab_sso_salt }}",
	"newrelic_key": "{{ newrelic_key }}",
	"bugsnag_key": "{{ bugsnag_key }}",
	"stripe": {
		"pub": "{{ stripe_pub }}",
		"secret": "{{ stripe_secret }}"
	},
	"smtp": {
		"user": "{{ smtp_user }}",
		"pass": "{{ smtp_pass }}"
	},
	"db": "mongodb://{{ mongodb_auth_user }}:{{ mongodb_auth_pass }}@{{ mongo_ip }}:2017/ng",
	"networkDb": "mongodb://{{ mongodb_auth_network_user }}:{{ mongodb_auth_network_pass }}@{{ mongo_ip }}:2017/ngNetwork",
	"db_options": {
		"auto_reconnect": true,
		"native_parser": true,
		"server": {
			"auto_reconnect": true
		}
	},
	"networkDb_options": {
		"auto_reconnect": true,
		"native_parser": true,
		"server": {
			"auto_reconnect": true
		}
	},
	"port": 80,
	"cdn": {
		"enabled": false,
		"url": ""
	},
	"admin_mysql": {
		"host": "{{ admin_mysql_ip }}",
		"ip": "{{ admin_mysql_ip }}",
		"user": "{{ admin_mysql_user }}",
		"password": "{{ admin_mysql_pass }}"
	},
	"admin_mongodb": {
		"host": "{{ admin_mongodb_ip }}",
		"ip": "{{ admin_mongodb_ip }}",
		"user": "{{ admin_mongodb_user }}",
		"pass": "{{ admin_mongodb_pass }}"
	}
}